    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
