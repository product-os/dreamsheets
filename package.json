{
  "name": "@balena/gsheet-utils",
  "version": "0.1.0",
  "description": "Common Apps Script / JS / TS functions used by balena data models in GSheet",
  "homepage": "https://github.com/product-os/gsheet-utils#readme",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "balena",
    "typescript"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/product-os/gsheet-utils.git"
  },
  "bugs": {
    "url": "https://github.com/product-os/gsheet-utils/issues"
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "npm run lint && tsc",
